@using Aiwins.Rocket.AspNetCore.Mvc.UI.Theme.Shared.Demo.Views.Components.Themes.Shared.Demos.ProgressBarsDemo

<rocket-component-demo-section title="Example" view-path="@ProgressBarsDemoViewComponent.ViewPath">
    <rocket-progress-bar value="70" />

    <rocket-progress-bar type="Warning" value="25"> %25 </rocket-progress-bar>

    <rocket-progress-bar type="Success" value="40" strip="true" />

    <rocket-progress-bar type="Dark" value="10" min-value="5" max-value="15" strip="true"> %50 </rocket-progress-bar>

    <rocket-progress-group>
        <rocket-progress-part type="Success" value="25" />
        <rocket-progress-part type="Danger" value="10" strip="true"> %10 </rocket-progress-part>
        <rocket-progress-part type="Primary" value="50" animation="true" strip="true" />
    </rocket-progress-group>
</rocket-component-demo-section>
