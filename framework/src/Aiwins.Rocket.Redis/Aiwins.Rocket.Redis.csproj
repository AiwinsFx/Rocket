  <ItemGroup>
    <PackageReference Include="StackExchange.Redis" Version="2.1.30" />
    <ProjectReference Include="..\Aiwins.Rocket.Json\Aiwins.Rocket.Json.csproj" />
    <ProjectReference Include="..\Aiwins.Rocket.Threading\Aiwins.Rocket.Threading.csproj" />
  </ItemGroup>

</Project>
